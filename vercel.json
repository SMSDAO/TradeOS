{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd webapp && npm run build",
  "devCommand": "cd webapp && npm run dev",
  "installCommand": "cd webapp && npm install",
  "framework": "nextjs",
  "outputDirectory": "webapp/.next",
  "regions": [
    "iad1"
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs22.x",
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
